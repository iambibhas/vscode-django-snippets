{
    "name": "django-snippets",
    "displayName": "Django Snippets",
    "description": "Common Django snippets for everyday use",
    "version": "1.1.0",
    "publisher": "bibhasdn",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/iambibhas/vscode-django-snippets.git"
    },
    "bugs": {
        "url": "https://github.com/iambibhas/vscode-django-snippets/issues"
    },

    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "python",
                "path": "./snippets/django-snippets.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}
